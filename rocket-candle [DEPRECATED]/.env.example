# Rocket Candle Environment Configuration
# Copy this file to .env and update the values

# Blockchain Configuration
VITE_MONAD_RPC_URL=https://testnet-rpc.monad.xyz/
VITE_MONAD_EXPLORER_URL=https://testnet.monadexplorer.com/
VITE_MONAD_CHAIN_ID=10143

# Contract Addresses (Update with your deployed contract addresses)
VITE_ROCKET_CANDLE_CONTRACT=
VITE_ROCKET_FUEL_CONTRACT=
VITE_ROCKET_CANDLE_SCORES_CONTRACT=

# WalletConnect Configuration (Get from https://walletconnect.com/)
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# API Configuration
VITE_API_BASE_URL=http://localhost:4000/api

# Game Configuration
VITE_GAME_NAME=Rocket Candle
VITE_GAME_VERSION=1.0.0

# Development Configuration
NODE_ENV=development
VITE_DEBUG_MODE=true
